{
  "name": "cash",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "build-storybook": "build-storybook -s public",
    "chromatic": "npx chromatic",
    "csscomb": "csscomb src/",
    "eject": "react-scripts eject",
    "format": "prettier --write src",
    "lint": "tsc --noEmit && eslint --fix src/**/*.{ts,tsx} src/**/**/*.{ts,tsx}",
    "start": "react-scripts start",
    "storybook": "start-storybook",
    "test": "react-scripts test",
    "test:e2e": "react-scripts test src/tests/e2e",
    "test:unit": "react-scripts test src/tests/unit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "*.{css}": [
      "csscomb --tty-mode --"
    ],
    "*.{ts,tsx}": [],
    "package.json": [
      "sort-package-json"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./config/git/cz-config.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "resolutions": {
    "babel-loader": "8.1.0"
  },
  "dependencies": {
    "@babel/helper-call-delegate": "^7.11.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@storybook/addon-backgrounds": "^6.2.9",
    "@testing-library/react": "^11.0.2",
    "@types/react-redux": "^7.1.11",
    "classnames": "^2.2.6",
    "puppeteer": "^5.2.1",
    "react": "^16.14.0",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.2",
    "react-scripts": "3.4.3",
    "redux": "^4.0.5",
    "reselect": "^4.0.0",
    "storybook-addon-pseudo-states": "^1.0.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.12.1",
    "@storybook/addon-docs": "6.1.11",
    "@storybook/react": "6.1.11",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/user-event": "^12.6.0",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.6.4",
    "@types/puppeteer": "^3.0.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.11.1",
    "chromatic": "^5.5.0",
    "commitizen": "^4.2.2",
    "csscomb": "^4.3.0",
    "cz-customizable": "^6.3.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "4.0.0",
    "husky": "^4.2.5",
    "jest-junit": "^12.0.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.1.2",
    "sort-package-json": "^1.44.0",
    "storybook-css-modules-preset": "^1.0.8",
    "typescript": "^4.0.2"
  }
}
