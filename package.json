{
  "name": "cash",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "format": "prettier --write src",
    "lint": "tsc --noEmit && eslint --fix src/**/*.{ts,tsx} src/**/**/*.{ts,tsx}",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:e2e": "react-scripts test src/**/*.e2e.test.ts",
    "test:unit": "react-scripts test src/**/**/*.spec.ts"
  },
  "lint-staged": {
    "*.{css}": [
      "csscomb --tty-mode --"
    ],
    "*.{ts,tsx}": [],
    "package.json": [
      "sort-package-json"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@babel/helper-call-delegate": "^7.12.3",
    "@testing-library/react": "^13.3.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/user-event": "^14.4.1",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest-junit": "^14.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "classnames": "^2.3.1"
  }
}
