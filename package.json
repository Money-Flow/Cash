{
  "name": "cash",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "csscomb": "csscomb src/",
    "eject": "react-scripts eject",
    "lint:fix": "prettier --write",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:e2e": "react-scripts test src/**/*.e2e.test.js",
    "test:unit": "react-scripts test src/**/**/*.spec.{js,ts}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,scss}": [
      "csscomb --tty-mode --"
    ],
    "*.{js,jsx,tsx}": [
      "yarn lint:fix"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "react": "^16.13.0",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.0"
  },
  "devDependencies": {
    "@babel/helper-call-delegate": "^7.8.7",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "@testing-library/user-event": "^10.0.1",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@types/uuid": "^7.0.2",
    "classnames": "^2.2.6",
    "csscomb": "^4.3.0",
    "husky": "^4.2.5",
    "jest-junit": "^10.0.0",
    "lint-staged": "^10.1.2",
    "prettier": "2.0.2",
    "puppeteer": "^2.1.1",
    "sort-package-json": "^1.40.0",
    "typescript": "^3.8.3",
    "uuid": "^7.0.3"
  }
}
