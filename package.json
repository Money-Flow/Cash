{
  "name": "cash",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "csscomb": "csscomb src/",
    "eject": "react-scripts eject",
    "lint:fix": "prettier --write",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,scss}": [
      "csscomb --tty-mode --"
    ],
    "*.{js,jsx}": [
      "yarn lint:fix"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-scripts": "3.4.0"
  },
  "devDependencies": {
    "husky": "^4.2.1",
    "jest-junit": "^10.0.0",
    "lint-staged": "^10.0.9",
    "prettier": "^1.19.1",
    "sort-package-json": "^1.40.0"
  }
}
